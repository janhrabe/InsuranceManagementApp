@model InsuranceManagementApp.Models.ContactForm

@{
    ViewData["Title"] = "Details";
}

<h2>Kontkatní Formulář</h2>

<div>
    <h4>Detail</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.FullName)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.FullName)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Email)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Email)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Message)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Message)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.DateSubmitted)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DateSubmitted)
        </dd>
    </dl>
</div>
<div>
    <a class="btn" asp-action="Edit" asp-route-id="@Model?.Id">Upravit</a> |
    <a class="btn" asp-action="Index">Zpět na výpis</a>
</div>
